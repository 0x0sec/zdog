{
  "extends": "../.eslintrc.json",
  "env": {
    "browser": false
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "globals": {
    "console": "readonly"
  },
  "rules": {
    "callback-return": "error",
    "global-require": "error",
    "handle-callback-err": "error",
    "id-length": [ "error", {
      "min": 2,
      "max": 30
    }],
    "max-params": [ "error", {
      "max": 4
    }],
    "no-console": "off",
    "no-var": "error"
  }
}
